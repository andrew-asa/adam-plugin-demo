{
    "include": [
      "src/renderer/src/env.d.ts",
      "src/renderer/src/**/*",
      "src/renderer/**/*.vue",
    ],
    "compilerOptions": {
      "composite": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ],
        "@renderer/*": [
          "src/renderer/*"
        ],
        "@common/*": [
          "src/common/*"
        ],
      },
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "strict": false,
    }
  }